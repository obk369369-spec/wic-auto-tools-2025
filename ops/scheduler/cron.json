{
  "jobs": [
    {
      "id": "report-hourly-upload",
      "command": "deno run -A /ops/report/report_loop_upload.ts",
      "schedule": "0 * * * *",
      "timezone": "Asia/Seoul",
      "stdout": "/ops/logs/report-hourly.log",
      "stderr": "/ops/logs/report-hourly.err"
    },
    {
      "id": "export-latest-sync",
      "command": "deno run -A /ops/export_latest.ts",
      "schedule": "1 * * * *",
      "timezone": "Asia/Seoul",
      "stdout": "/ops/logs/export.log",
      "stderr": "/ops/logs/export.err"
    }
  ]
}
